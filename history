;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(yes-or-no-p-history file-name-history grep-files-history grep-regexp-history extended-command-history))
(setq yes-or-no-p-history '("yes"))
(setq file-name-history '("~/.emacs.d/"))
(setq grep-files-history '("all"))
(setq grep-regexp-history '("haml"))
(setq extended-command-history '("rgrep"))
